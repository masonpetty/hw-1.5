using System;

class Program
{
    static void Main()
    {
        // Given values for X and Y
        double X = 2.5;
        double Y = 3.3;

        // Calculate the value of Z using the linear equation
        double Z = 4 * X * X + 3 * Y;

        // Print the result in the specified format
        Console.WriteLine($"For X = {X} and Y = {Y}, the value of Z is: {Z:F2}");
    }
}
